// InternetGateway
schema InternetGateway {
    string vpcId              // The VPC ID to attach the internet gateway to
    map tags               // Tags to apply to the internet gateway
    @cloud
    string internetGatewayId  // The internet gateway ID assigned by AWS
    @cloud
    string ownerId            // The AWS account ID that owns the internet gateway
    @cloud
    string attachmentState    // Attachment state (available, attaching, detaching)
}
